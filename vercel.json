{
  "version": 2,
  "public": true,
  "name": "claude-code-notifications",
  "alias": ["claude-code-notifications.vercel.app"],
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/github",
      "destination": "https://github.com/dongzhenye/claude-code-notifications"
    }
  ]
}